package Main;

public class Main {

	public static void main(String[] args) {
		int game[] = new int[3];
		
		System.out.println("------------------------------");
		System.out.println("야구게임 시작, 랜덤번호 생성");
		System.out.println("------------------------------");
		
		for (int i=0; i!=3; i++) {
			int num = (int)(Math.random()*10)+1;
			
			if (num != 10) {
				game[i] = num;
				System.out.print(game[i]);
			}
			
		}
	}
}