package Main;

public class Main {
	
	public static void main(String[] args) {
		System.out.println("------------------------------");
		System.out.println("야구게임 시작, 랜던번호 생성");
		System.out.println("------------------------------");
		
		for (int i=0; i==3; i++) {
			System.out.println("야구게임 시작, 랜던번호 생성1");
		}
		
		int firstNum = (int)(Math.random()*10000)%10;
		int secondNum = (int)(Math.random()*10000)%10;
		int thirdNum = (int)(Math.random()*10000)%10;
		
		
		
		
		
		System.out.println(firstNum);
		System.out.println(secondNum);
		System.out.println(thirdNum);
	}
}